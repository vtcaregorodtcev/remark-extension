{
  "name": "@vtcaregorodtcev/remark-extension",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "start": "npm-run-all --parallel 'start:*'",
    "start:css": "tailwindcss -i src/**/*.css -w",
    "start:popup": "parcel src/Popup/popup.html --open",
    "build:popup": "parcel build src/Popup/popup.html",
    "build:background": "parcel build src/background.ts",
    "build": "rm -rf dist && npm-run-all 'build:*' && node ./scripts/build-manifest",
    "test": "jest --detectOpenHandles --verbose --testPathPattern=.*.test.ts"
  },
  "license": "MIT",
  "dependencies": {
    "lodash.noop": "^3.0.1",
    "lodash.uniq": "^4.5.0",
    "solid-js": "^1.3.7"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@parcel/core": "^2.3.2",
    "@parcel/transformer-sass": "^2.3.2",
    "@svgr/parcel-plugin-svgr": "^5.5.0",
    "@types/chrome": "^0.0.175",
    "@types/jest": "^27.4.0",
    "@types/lodash.noop": "^3.0.6",
    "@types/lodash.uniq": "^4.5.6",
    "@types/node": "^17.0.18",
    "autoprefixer": "^10.4.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-preset-solid": "^1.3.6",
    "cssnano": "^5.0.17",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.3.2",
    "parcel-plugin-nuke-dist": "^1.0.1",
    "postcss": "^8.4.6",
    "postcss-import": "^14.0.2",
    "puppeteer": "^13.3.2",
    "tailwindcss": "^3.0.23",
    "ts-jest": "^27.1.3",
    "typed-scss-modules": "^4.1.2",
    "typescript": "^4.5.5"
  }
}
